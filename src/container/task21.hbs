<main>
    <div class="card">
        <div class="card-body">


            <div class="card">
                <div class="card-body">
                    {{#with heading/main}}
                    <h1>{{title}}</h1>
                    <p>{{text}}</p>
                    <a href="#" class="btn btn-primary">
                        {{button_text/solid}}
                    </a>
                    <br>
                    <br>
                    <a href="#" class="btn btn-outline-primary">
                        {{button_text/outline}}
                    </a>

                    {{/with}}

                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with heading/second}}
                    <h2>{{title}}</h2>
                    <p>{{text}}</p>
                    <a href="#" class="btn btn-secondary">
                        {{button_text/solid}}
                    </a>
                    <br>
                    <br>
                    <a href="#" class="btn btn-outline-secondary">
                        {{button_text/outline}}
                    </a>


                    {{/with}}
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with heading/subblock}}
                    <h3>{{title}}</h3>
                    <p>{{text}}</p>
                    <a href="#" class="btn btn-dark">
                        {{button_text/solid}}
                    </a>
                    <br>
                    <br>
                    <a href="#" class="btn btn-outline-dark">
                        {{button_text/outline}}
                    </a>


                    {{/with}}
                </div>
            </div>


        </div>
    </div>
    <br>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with nested_display}}
            <div class="display-1">{{title}}</div>
            <div class="card">
                <div class="card-body">
                    <div class="display-4">{{children_display/title}}</div>
                    <br>
                    <p>{{children_display/text}}</p>
                    <br>
                    <a href="#" class="btn btn-outline-warning">
                        {{button/text}}
                    </a>
                </div>
            </div>
            <br>
            <h6>{{header_text}}</h6>
            <br>
            <a href="#" class="btn btn-info text-light">
                {{button/text}}

            </a>





            {{/with}}

        </div>
    </div>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with paragraph}}
            <p class="text-centr text-uppercase">{{title}}</p>
            <p>{{text}}</p>
            <a href="https://google.com" class="btn btn-link">
                {{button/text}}
            </a>


            {{/with}}

            <br>
            <br>
            <div class="card">
                <div class="card-body">

                    <ul>

                        {{#each paragraph/list_name}}
                        <br>
                        <li><a href="{{url}}" target="_blank">{{text}}</a></li>

                        {{/each}}
                    </ul>
                </div>
            </div>

        </div>
    </div>


</main>